@model IEnumerable<HomeSurveillanceApp.Models.Telemetry>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ActivityTimeStamp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IOUnit)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ActivityTimeStamp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IOUnit.IOUnitId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TelemetryId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TelemetryId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TelemetryId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
