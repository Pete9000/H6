@model HomeSurveillanceApp.Models.Telemetry

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Telemetry</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="TelemetryId" />
            <div class="form-group">
                <label asp-for="ActivityTimeStamp" class="control-label"></label>
                <input asp-for="ActivityTimeStamp" class="form-control" />
                <span asp-validation-for="ActivityTimeStamp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IOUnitId" class="control-label"></label>
                <select asp-for="IOUnitId" class="form-control" asp-items="ViewBag.IOUnitId"></select>
                <span asp-validation-for="IOUnitId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
